{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "dom",
      "es7",
      "es2017.object"
    ],
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "sourceMap": true,
    "outDir": "dist/",
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules"
  ]
}